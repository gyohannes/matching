<caption>Applicants program choice statstics</caption><br/>
<table class='table table-bordered'>
<thead>
<tr>
	<th>Program Name</th>
	<th>1st Choice</th>
	<th>2nd Choice</th>
	<th>3rd Choice</th>
</tr>
</thead>
<% @programs.each do |p| %>
	<tr>
	<td><%= p %></td>
	<td><%= p.count_applicants(1) %></td>
	<td><%= p.count_applicants(2) %></td>
	<td><%= p.count_applicants(3) %></td>
	</tr>
<% end %>
</table><br/><br/>
<center><%= button_to 'Match', match_placements_path, {class: "btn btn-primary"} %></center>
